{
    "name": "flyo/nitro-laravel",
    "description": "Flyo Nitro Laravel Framework Module",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flyo\\Laravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Basil Suter",
            "email": "git@nadar.io"
        }
    ],
    "require": {
        "flyo/nitro-php" : "^1.0",
        "illuminate/support": "^11.0",
        "illuminate/view": "^11",
        "laravel/helpers": "^1.6",
        "flyo/nitro-php-bridge": "^1.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flyo\\Laravel\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
